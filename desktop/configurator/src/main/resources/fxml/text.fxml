<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<AnchorPane prefHeight="31.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.fullrest.mfr.configurator.controller.TextFieldController">
    <children>
        <HBox prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <TextField fx:id="textField" HBox.hgrow="ALWAYS"/>
                <Button fx:id="okButton" defaultButton="true" mnemonicParsing="false" text="OK"/>
                <Button cancelButton="true" mnemonicParsing="false" onAction="#close" text="Cancel"/>
            </children>
        </HBox>
    </children>
</AnchorPane>
