                                                <Button mnemonicParsing="false" onAction="#hide" text="Закрыть"/>
                                            </children>
                                        </VBox>
                                    </children>
                                    <VBox.margin>
                                        <Insets/>
                                    </VBox.margin>
                                </VBox>
                            </children>
                            <HBox.margin>
                                <Insets/>
                            </HBox.margin>
                        </VBox>
                    </children>
                    <VBox.margin>
                        <Insets top="162.0"/>
                    </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
    <styleClass>
        <String fx:value="configWindow"/>
        <String fx:value="progress"/>
    </styleClass>
</AnchorPane>
