                        </VBox>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                            <styleClass>
                                <String fx:value="right"/>
                                <String fx:value="item"/>
                            </styleClass>
                        </VBox>
                    </children>
                    <styleClass>
                        <String fx:value="configHeader"/>
